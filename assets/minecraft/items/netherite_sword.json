{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:component",
    "component": "minecraft:custom_name",
    "cases": [
      {
        "when": {
          "text": "アイス・ソード",
          "bold": true,
          "italic": false
        },
        "model": {
          "type": "minecraft:model",
          "model": "dekitate_event:item/ice/ice_sword"
        }
      },
      {
        "when": {
          "text": "天叢雲剣",
          "color": "yellow",
          "bold": true,
          "italic": false
        },
        "model": {
          "type": "minecraft:model",
          "model": "dekitate_event:item/yomi/amakumo_ken"
        }
      },
      {
        "when": {
          "text": "ランク",
          "color": "white",
          "bold": true,
          "italic": false
        },
        "model": {
          "type": "minecraft:model",
          "model": "dekitate_event:item/dpass/rank_pro"
        }
      },
      {
        "when": {
          "text": " ",
          "bold": true,
          "italic": false,
          "extra": [
            {
              "text": "PRO",
              "color": "dark_red",
              "bold": true
            },
            {
              "text": " ランク",
              "color": "white"
            }
          ]
        },
        "model": {
          "type": "minecraft:model",
          "model": "dekitate_event:item/dpass/rank_pro"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:range_dispatch",
      "property": "minecraft:custom_model_data",
      "entries": [
        {
          "threshold": 1,
          "model": {
            "type": "model",
            "model": "custom/dungeon/katana"
          }
        },
        {
          "threshold": 3,
          "model": {
            "type": "model",
            "model": "custom/2023_summer/apple_sword"
          }
        },
        {
          "threshold": 4,
          "model": {
            "type": "model",
            "model": "custom/private_event/neko_pvp_sword"
          }
        },
        {
        "threshold": 5,
        "model": {
          "type": "model",
          "model": "custom/2025_winter/ice_sword"
          }
        },
        {
        "threshold": 6,
        "model": {
          "type": "model",
          "model": "minecraft:item/netherite_sword"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/netherite_sword"
      }
    }
  }
}